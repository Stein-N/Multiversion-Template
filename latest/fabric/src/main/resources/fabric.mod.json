{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [ "${mod_author}" ],
  "contact": {
    "homepage": "https://github.com/Stein-N/Resource-Backpacks",
    "issues": "https://github.com/Stein-N/Resource-Backpacks/issues"
  },
  "license": "${mod_license}",
  "icon": "icon.png",
  "environment": "*",
  "accessWidener" : "${mod_id}.accesswidener",
  "entrypoints": {
    "main": [ "net.xstopho.example.Example" ],
    "client": [ "net.xstopho.example.ExampleClient" ]
  },
  "mixins": [ "${mod_id}.mixins.json" ],
  "depends": {
    "fabric-api": ">=${fabric_api}",
    "fabricloader": ">=${fabric_loader}",
    "minecraft": "${minecraft_version}",
    "java": ">=${java_version}"
  },
  "custom": {
    "modmenu": {
      "update-checker": true
    }
  }
}